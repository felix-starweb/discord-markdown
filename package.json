{
  "type": "module",
  "name": "@felixrydberg/discord-markdown",
  "version": "1.1.4",
  "description": "A markdown parser that matches Discords markdown spec.",
  "main": "./dist/index.js",
  "source": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "styles": "./dist/style.css",
  "files": [
    "./dist",
    "./src",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "style": "./dist/style.css",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./dist": {
      "style": "./style.css",
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.umd.cjs"
    }
  },
  "scripts": {
    "build": "npx vite build",
    "tests": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "discord",
    "discord markdown",
    "markdown",
    "simple markdown",
    "simplemarkdown"
  ],
  "repository": "https://github.com/felixrydberg/discord-markdown.git",
  "author": "felixrydberg",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@khanacademy/simple-markdown": "^0.10.2",
    "highlight.js": "^11.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue": "^3.3.4"
  }
}
